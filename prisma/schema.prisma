datasource db {
  provider = "postgresql"
  url      = "postgres://admin:password@localhost:5432/test?schema=public"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialActions"]
}

model Quote {
  id                    String    @id @default(cuid())
  text                  String
  author                String?
}